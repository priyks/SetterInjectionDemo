<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
 http://www.springframework.org/schema/beans/spring-beans.xsd">
	<!-- this is first been -->
	<bean name="engine1"
		class="com.springcore.ref.Engine">
		<property name="name" value="SI Engine" />

		<property name="capacity" value="109.51" />
	</bean>

	<!-- this is third been using p schema -->
	<bean name="bike" class="com.springcore.ref.Bike" p:brandName="Activa 6G" p:engine-ref="engine1" p:millage="55.5f" p:price="77889"/>

</beans>
 