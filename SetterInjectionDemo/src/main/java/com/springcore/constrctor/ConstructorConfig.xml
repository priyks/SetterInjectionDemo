<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
 http://www.springframework.org/schema/beans/spring-beans.xsd">
	<!-- this is first been -->
	<bean name="certificate"
		class="com.springcore.constrctor.Certificate">
	<constructor-arg name="name" value="python 3 complete certification"/>
	<constructor-arg name="duration" value="5" type="int"></constructor-arg>
		
	</bean>

	<!-- this is second been -->
	<bean name="person"
		class="com.springcore.constrctor.Person">
		<constructor-arg name="name" value="Rushikesh Kulkarni"/>
		<constructor-arg name="age" value="22"/>
		<constructor-arg name="certi" ref="certificate"/>
		
	</bean>

	

</beans>
 