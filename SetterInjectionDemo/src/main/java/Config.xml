<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
 http://www.springframework.org/schema/beans/spring-beans.xsd">
	<!-- this is first been -->
	<bean name="Student1"
		class="com.springcore.SetterInjectionDemo.Student">
		<property name="studentId" value="101" />

		<property name="studentName" value="Priyanka" />

		<property name="address" value="Pune" />
	</bean>

	<!-- this is second been -->
	<bean name="Student2"
		class="com.springcore.SetterInjectionDemo.Student">
		<property name="studentId" value="102" />

		<property name="studentName" value="Omkar" />

		<property name="address" value="Aurangabad" />
	</bean>

	<!-- this is third been using p schema -->
	<bean name="Student3"
		class="com.springcore.SetterInjectionDemo.Student" p:studentId="103" p:studentName="Rushikesh" p:address="Beed"/>

</beans>
 